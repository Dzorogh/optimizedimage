{
  "name": "dzorogh/optimizedimage",
  "description": "Optimize images on fly, generate webp, resize, etc.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Sergey Indenbom",
      "email": "dzorogh@gmail.com"
    }
  ],
  "require": {
    "php" : "^7.0",
    "league/glide": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Dzorogh\\OptimizedImage\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dzorogh\\OptimizedImage\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dzorogh\\OptimizedImage\\ImageServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^5.0",
    "phpunit/phpunit": "^9.4"
  }
}
